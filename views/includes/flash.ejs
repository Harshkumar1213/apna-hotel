<% if(msgSuccess && msgSuccess.length){%>
<div class="alert alert-success col-6 offset-3 mt-4 alert-dismissible fade show" role="alert">
  <%=msgSuccess%>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert" 
    aria-label="Close"
  ></button>
</div>
<%}%>

<% if(error && error.length){%>
  <div class="alert alert-danger col-6 offset-3 mt-4 alert-dismissible fade show" role="alert">
    <%=error%>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
  <%}%>
